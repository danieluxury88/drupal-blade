{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.6",
        "drupal/advent_calendar": "^1.0@beta",
        "drupal/ai": "^1.1",
        "drupal/backup_migrate": "^5.1",
        "drupal/book": "^2.0",
        "drupal/ckeditor_ai_agent": "^1.0@beta",
        "drupal/coffee": "^2.0",
        "drupal/commerce": "^3.0",
        "drupal/config_inspector": "^2.1",
        "drupal/config_split": "^2.0",
        "drupal/core-composer-scaffold": "^11.1",
        "drupal/core-project-message": "^11.1",
        "drupal/core-recommended": "^11.1",
        "drupal/devel": "^5.3",
        "drupal/drd": "^4.1",
        "drupal/entity_browser": "^2.13",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_group": "^4.0",
        "drupal/forum": "^1.0",
        "drupal/gin": "^5.0",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.2",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/mce": "^1.0",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/metatag": "^2.1",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.13",
        "drupal/physical": "^1.5",
        "drupal/poll": "^2.0@alpha",
        "drupal/pwa": "^2.1@beta",
        "drupal/redirect": "^1.11",
        "drupal/scheduler": "^2.2",
        "drupal/seckit": "^2.0",
        "drupal/simple_sitemap": "^4.2",
        "drupal/site_audit": "^4.1",
        "drupal/smart_date": "^4.2",
        "drupal/svg_image_field": "^2.3",
        "drupal/trash": "^3.0",
        "drupal/twig_tweak": "^3.4",
        "drupal/type_tray": "^1.3",
        "drupal/upgrade_status": "^4.3",
        "drupal/webform": "^6.3@beta",
        "drush/drush": "^13.6",
        "seriousdesign/project_overview": "dev-main"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
            "drupal/advent_calendar": {
                "Handle SDC dependency and optional config install": "patches/local/advent_calendar-fixes.patch"
            }
        }
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^11.2",
        "drupal/webprofiler": "^11.1",
        "squizlabs/php_codesniffer": "^3.13"
    }
}
